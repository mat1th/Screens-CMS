 {{> nav}}
<section class="content scrollable">
    <section class="content-body">
        <h1>{{data.diaId}} </h1>
    </section>
    <section class="content-body">
        <article class="">
            <h2>Discription</h2>
            <img src="/download/{{data.filename}}" alt="" />
            <p>
                {{data.discription}}
            </p>
        </article>
        <table>
            <tr>
                <th>Duration</th>
                <td>{{data.duration}}</td>
            </tr>
            <tr>
                <th>Animation</th>
                <td>{{data.animation}}</td>
            </tr>
            <tr>
                <th>type</th>
                <td>{{data.type}}</td>
            </tr>
            <tr>
                <th>date_begin</th>
                <td>{{data.dateStart}}</td>
            </tr>
            <tr>
                <th>date_end</th>
                <td>{{data.dateEnd}}</td>
            </tr>
            <tr>
                <th>dataCreated</th>
                <td>{{data.dataCreated}}</td>
            </tr>
        </table>
        <p>
            {{data.diaDiscription}} {{data.diaduration}}
        </p>
    </section>
</section>
